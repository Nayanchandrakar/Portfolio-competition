datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model projects {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  projectName String
  image       String
  projectLink String
}

model resources {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  resourceName String
  image        String
  redirectUrl  String
}

model socialUrls {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  linkedin  String
  instagram String?
  facebook  String?
  youtube   String?
  twitter   String?
  slack     String?
  github    String
}
